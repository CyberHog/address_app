<h1>住所自動入力</h1>

<%= simple_form_for @user do |f| %>


<%= f.input :last_name %>
<%= f.input :first_name %>
<%= f.input :phone %>
<%= f.input :postcode, input_html:{id:"address_zipcode"} %>
<%= f.input :prefecture, input_html:{id:"address_prefecture_name"} %>
<%= f.input :city, input_html:{id:"address_city"} %>
<%= f.input :address_street, input_html:{id:"address_street"} %>
<%= f.input :address_building, input_html:{id:"address_building"} %>

<%= f.button :submit %>

<% end %>
